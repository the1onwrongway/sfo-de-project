{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sfo-adf"
		},
		"sfo_linked_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sfo_linked_service'"
		},
		"ls_databricks_sfo_analytics_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks_sfo_analytics'"
		},
		"sfo_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sfostorageadl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sfo-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sfo_linked_service_accountKey": {
						"value": "[parameters('sfo_linked_service_accountKey')]"
					},
					"ls_databricks_sfo_analytics_accessToken": {
						"value": "[parameters('ls_databricks_sfo_analytics_accessToken')]"
					},
					"sfo_linked_service_properties_typeProperties_url": {
						"value": "[parameters('sfo_linked_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}