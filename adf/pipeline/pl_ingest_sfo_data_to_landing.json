{
	"name": "pl_ingest_sfo_data_to_landing",
	"properties": {
		"activities": [
			{
				"name": "Set Execution ID",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "execution_id",
					"value": {
						"value": "@concat(pipeline().Pipeline, '_', utcNow('yyyyMMddHHmmss'))",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set Pipeline Start Time",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Set Execution ID",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "pipeline_start_time",
					"value": {
						"value": "@utcNow()",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Run incremental loading Databricks notebook",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "Set Pipeline Start Time",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/sfo-analytics/01_incremental_loading",
					"baseParameters": {
						"start_date": "@pipeline().parameters.start_date",
						"dataset_filter": "@pipeline().parameters.dataset_filter",
						"load_type": "@pipeline().parameters.load_type"
					}
				},
				"linkedServiceName": {
					"referenceName": "ls_databricks_sfo_analytics",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"start_date": {
				"type": "string",
				"defaultValue": "202001"
			},
			"dataset_filter": {
				"type": "string",
				"defaultValue": "all"
			},
			"load_type": {
				"type": "string",
				"defaultValue": "incremental"
			}
		},
		"variables": {
			"execution_id": {
				"type": "String"
			},
			"pipeline_start_time": {
				"type": "String"
			}
		},
		"annotations": []
	}
}